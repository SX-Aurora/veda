{
	"name":			"veda",
	"version":		"@VEDA_VERSION@",
	"platform":		"linux_x86_64",
	"summary":		"VEDA",
	"readme":		"veda/README.md",
	"author":		"__GIT__",
	"author-email":		"__GIT__",
	"license":		"3 BSD-License",
	"license-file": 	"veda/LICENSE",
	"requires-python":	">=3.6",
	"download-url":		"https://github.com/sx-aurora/veda/tags",
	"classifier": [
		"License :: OSI Approved :: BSD License",
		"Programming Language :: Python :: 3 :: Only",
		"Programming Language :: C"
	],
	"payload": [
		"veda/bin/veda-smi?",
		"veda/include/vera/*.h?",
		"veda/*_LICENSE",
		"veda/cmake/*.cmake",
		"veda/cmake/nec/*.cmake",
		"veda/cmake/nec/*.cmake.in",
		"veda/cmake/nec/inject/*.cmake",
		"veda/include/*.h",
		"veda/include/veda/*.h",
		"veda/include/veda/cpp/*.h",
		"veda/lib64/libveda.so*",
		"veda/lib64/libveo.so*",
		"veda/libve/libveda.vso*",
		"veda/libexec/aveorun_ve*",
		"veda/libexec/aveorun-ftrace_ve*"
	],
	"requires-dist": [
		"tungl (>= @Tungl_VERSION@, < @Tungl_VERSION_NEXT_MINOR@)"
	]
}